<?xml version="1.0">

<simulation id="fire">
    <head>
        <author>Angela and George Shiflet</author>
        <title>Spreading of Fire</title>
    </head>

    <dim>
        <width>50</width>
        <height>50</height>
    </dim>
    
    <params>
        <prob>0.5</prob>
    </params>
    
    <cells>
        <cell state="BURNING">
            <x>0</x>
            <y>0</y>
        </cell>
        <cell state="EMPTY">
            <x>1</x>
            <y>1</y>
        </cell>
        <cell state="TREE">
            <x>2</x>
            <y>1</y>
        </cell>
    </cell>
</simulation>