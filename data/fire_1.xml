<?xml version="1.0" encoding="UTF-8"?>

<data simulation="fire">
    <head>
        <author>Angela and George Shiflet</author>
        <title>Spreading of Fire</title>
    </head>

    <dim>
        <width>80</width>
        <height>80</height>
    </dim>
    
    <prob>0.5</prob>
    
    <cells>
        <cell state="BURNING">
            <x>0</x>
            <y>0</y>
        </cell>
        <cell state="EMPTY">
            <x>1</x>
            <y>1</y>
        </cell>
        <cell state="TREE">
            <x>2</x>
            <y>1</y>
        </cell>
    </cells>
</data>